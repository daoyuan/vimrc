snippet li "library()"
library(${1:})
endsnippet

snippet if "If Condition"
if(${1:condition}){
  ${2:}
}
endsnippet

snippet el "else"
else {
  ${1:}
}
endsnippet

snippet wh "while"
while(${1:condition}){
  ${2:}
}
endsnippet

snippet for "For Loop"
for(${1:i} in ${2:range}){
  ${3:}
}
endsnippet

snippet fun "Function"
${1:funname} <- function(${2:})
{
  ${3:}
}
endsnippet

snippet re "repeat"
repeat{
  ${2:}
  if(${1:condition}) break
}
endsnippet


